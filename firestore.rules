rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }

    match /xcompressor {
      match /global-stats/{statName=**} {
        allow get, create, update: if true;
      }

      match /website/users/{userId} {
        allow get, create, update: if true;
      }
    }
  }
}